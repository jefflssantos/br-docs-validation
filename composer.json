{
    "name": "jefflssantos/br-docs-validator",
    "description": "A Laravel Validator for Brazilian Documents",
    "keywords": ["laravel", "CNPJ", "CPF", "Validation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Santos",
            "email": "email@jeffsantos.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Jefflssantos\\BrDocsValidator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jefflssantos\\BrDocsValidator\\BrDocsValidatorServiceProvider"
            ]
        }
    }
}
